{
  "name": "image-resize-service",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
		"lint": "npm run typecheck && npm run eslint && npm run prettier",
		"lint:fix": "npm run typecheck && npm run eslint:fix && npm run prettier:fix",
		"eslint": "eslint \"**/*.{cjs,mjs,js,ts,tsx,md}\"",
		"eslint:fix": "eslint \"**/*.{cjs,mjs,js,ts,tsx,md}\" --fix",
		"prettier": "prettier --list-different \"src/**/*.{cjs,mjs,js,ts,tsx,md}\"",
		"prettier:fix": "prettier --write \"src/**/*.{cjs,mjs,js,ts,tsx,md}\"",
		"typecheck": "tsc --noemit",
		"prepare": "husky install .husky"
  },
  "author": "Matt DeKrey",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tsyringe": "^4.7.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.63",
    "nodemon": "^2.0.22"
  }
}
